{
  "name": "euphonia",
  "version": "1.0.0",
  "description": "An easy way to store and listen to your music",
  "scripts": {
    "build": "esbuild src/main/index.js --bundle --minify --sourcemap --outfile=js/index.js && esbuild src/tools/tools.js --bundle --minify --sourcemap --outfile=js/tools.js",
    "init": "npm i && composer i",
    "start": "php -S localhost:5151"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xwilarg/Euphonia.git"
  },
  "author": "Xwilarg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xwilarg/Euphonia/issues"
  },
  "homepage": "https://github.com/Xwilarg/Euphonia#readme",
  "devDependencies": {
    "@types/wanakana": "^4.0.3",
    "esbuild": "^0.14.18"
  },
  "dependencies": {
    "wanakana": "^4.0.2"
  }
}
