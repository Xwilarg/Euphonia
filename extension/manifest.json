{
    "name": "Euphonia Downloader",
    "description": "Easily download new musics for Euphonia",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "options_page": "options.html",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}